{
  "name": "alpha-quant",
  "version": "1.0.0",
  "description": "Alpha Quant - A comprehensive financial analysis web app with AI-powered insights and portfolio management",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "keywords": [
    "finance",
    "stocks",
    "ai",
    "portfolio",
    "analysis",
    "rag",
    "machine-learning",
    "fintech"
  ],
  "author": "Ugochukwu Belusochim",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.7.4",
    "xlsx": "^0.18.5",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
